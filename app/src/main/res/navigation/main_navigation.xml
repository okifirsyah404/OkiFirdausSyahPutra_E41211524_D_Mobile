<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.okifirsyah.okifirdaussyahputra_e41211524_gold_mobile.module.dashboard.HomeFragment"
        android:label="Sensor"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_gpsMapsFragment"
            app:destination="@id/gpsMapsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_proximitySensorFragment"
            app:destination="@id/proximitySensorFragment" />
        <action
            android:id="@+id/action_homeFragment_to_lightSensorFragment"
            app:destination="@id/lightSensorFragment" />
    </fragment>
    <fragment
        android:id="@+id/gpsMapsFragment"
        android:name="com.okifirsyah.okifirdaussyahputra_e41211524_gold_mobile.module.gps.GpsMapsFragment"
        android:label="Gps Maps"
        tools:layout="@layout/fragment_gps_maps" />
    <fragment
        android:id="@+id/proximitySensorFragment"
        android:name="com.okifirsyah.okifirdaussyahputra_e41211524_gold_mobile.module.proximity_sensor.ProximitySensorFragment"
        android:label="Proximity"
        tools:layout="@layout/fragment_proximity_sensor" />
    <fragment
        android:id="@+id/lightSensorFragment"
        android:name="com.okifirsyah.okifirdaussyahputra_e41211524_gold_mobile.module.light_sensor.LightSensorFragment"
        android:label="Light"
        tools:layout="@layout/fragment_light_sensor" />
</navigation>